<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="325.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ModifyPartController">
   <children>
      <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="322.0" prefWidth="400.0">
         <children>
            <Label layoutX="10.0" layoutY="14.0" text="Modify Part">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <RadioButton fx:id="inhousePartRadioButton" layoutX="150.0" layoutY="25.0" mnemonicParsing="false" onAction="#radioButtonChanged" selected="true" text="In-House" />
            <RadioButton fx:id="outsourcedPartRadioButton" layoutX="250.0" layoutY="25.0" mnemonicParsing="false" onAction="#radioButtonChanged" text="Outsourced" />
            <TextField fx:id="idTextField" editable="false" layoutX="150.0" layoutY="60.0" prefHeight="25.0" />
            <TextField fx:id="nameTextField" layoutX="150.0" layoutY="95.0" minHeight="25.0" />
            <TextField fx:id="stockTextField" layoutX="150.0" layoutY="130.0" minHeight="25.0" />
            <TextField fx:id="priceOrCostTextField" layoutX="150.0" layoutY="165.0" minHeight="25.0" />
            <TextField fx:id="maxTextField" layoutX="150.0" layoutY="200.0" minHeight="25.0" prefHeight="25.0" prefWidth="80.0" />
            <TextField fx:id="minTextField" layoutX="300.0" layoutY="200.0" minHeight="25.0" prefWidth="80.0" />
            <TextField fx:id="companyOrMachineTextField" layoutX="150.0" layoutY="235.0" minHeight="25.0" />
            <Label layoutX="60.0" layoutY="60.0" prefHeight="25.0" text="ID" />
            <Label layoutX="60.0" layoutY="95.0" prefHeight="25.0" text="Name" />
            <Label layoutX="60.0" layoutY="130.0" prefHeight="25.0" text="Inv" />
            <Label fx:id="priceOrCostLabel" layoutX="60.0" layoutY="165.0" prefHeight="25.0" text="Price/Cost" />
            <Label layoutX="60.0" layoutY="200.0" prefHeight="25.0" text="Max" />
            <Label layoutX="267.0" layoutY="200.0" prefHeight="25.0" text="Min" />
            <Label fx:id="companyOrMachineLabel" layoutX="60.0" layoutY="235.0" prefHeight="25.0" text="Company Name" />
            <Button fx:id="saveButton" layoutX="248.0" layoutY="280.0" mnemonicParsing="false" onAction="#saveButtonPushed" prefWidth="60.0" text="Save" />
            <Button fx:id="cancelButtonPushed" layoutX="320.0" layoutY="280.0" mnemonicParsing="false" onAction="#cancelButtonPushed" prefWidth="60.0" text="Cancel" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
